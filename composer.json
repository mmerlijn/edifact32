{
  "name": "mmerlijn/msg-edifact32",
  "description": "Dutch healthcare read/write edifact3.2 to/from repository",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "Menno",
      "email": "menno@mmerlijn.nl"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^8.1",
    "mmerlijn/msg-repo": "dev-master",
    "nesbot/carbon": "2.*"
  },
  "require-dev": {
    "phpunit/phpunit": "9.*"
  },
  "autoload": {
    "psr-4": {
      "mmerlijn\\msgEdifact32\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "mmerlijn\\msgEdifact32\\tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-f": "vendor/bin/phpunit --filter"
  },
  "config": {
    "sort-packages": true
  }
}
