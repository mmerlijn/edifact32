{
  "name": "mmerlijn/msg-edifact32",
  "description": "Dutch healthcare read/write edifact3.2 to/from repository",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "Menno",
      "email": "menno@mmerlijn.nl"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^8.2",
    "mmerlijn/msg-repo": "~2.0",
    "nesbot/carbon": "~3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0.1"
  },
  "autoload": {
    "psr-4": {
      "mmerlijn\\msgEdifact32\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "mmerlijn\\msgEdifact32\\tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-f": "vendor/bin/phpunit --filter"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.x-dev"
    }
  }
}
